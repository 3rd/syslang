{
  "name": "tree-sitter-syslang",
  "version": "0.1.0",
  "private": true,
  "main": "bindings/node",
  "types": "bindings/node",
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**"
  ],
  "highlights": [
    "queries/highlights.scm"
  ],
  "scripts": {
    "generate": "tree-sitter generate --abi 14",
